{
	"name": "stock-portfolio-web-typescript",
	"version": "1.0.0",
	"description": "A stock portfolio web application written in Gatsby JS",
	"scripts": {
		"start": "gatsby develop",
		"develop": "gatsby develop",
		"build": "gatsby build",
		"build:nodes": "rm -rf .cache && cd plugins/gatsby-source-portfolio && npx tsc --build tsconfig.json && cd ../..",
		"develop:clean": "npm run build:nodes && npm run develop",
		"type-check": "tsc --noEmit",
		"lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
		"prepare": "husky install"
	},
	"sideEffects": false,
	"dependencies": {
		"@primer/octicons-react": "^9.6.0",
		"@types/classnames": "^2.2.10",
		"@types/moment-timezone": "^0.5.13",
		"@types/numeral": "0.0.28",
		"@types/react-bootstrap-typeahead": "^3.4.6",
		"@types/react-paginate": "^6.2.1",
		"@types/react-redux": "^7.1.9",
		"axios": "^0.19.2",
		"bootstrap": "^4.5.0",
		"classnames": "^2.2.6",
		"colors": "^1.4.0",
		"crypto-browserify": "^3.12.0",
		"cryptr": "^6.0.2",
		"dotenv": "^16.0.3",
		"firebase": "^9.16.0",
		"firebase-admin": "^8.12.1",
		"firebaseui": "^6.0.2",
		"gatsby": "^3.0.0",
		"gatsby-plugin-sass": "^4.15.0",
		"moment-timezone": "^0.5.31",
		"node-html-parser": "^5.3.3",
		"number-precision": "^1.6.0",
		"numeral": "^2.0.6",
		"react": "^17.0.2",
		"react-append-to-body": "^2.0.25",
		"react-bootstrap-typeahead": "^5.0.0-rc.1",
		"react-dom": "^17.0.0",
		"react-paginate": "^7.0.0",
		"react-redux": "^7.2.0",
		"react-textarea-autosize": "^8.0.1",
		"react-typography": "^0.16.19",
		"redux": "^4.0.5",
		"rss-parser": "^3.12.0",
		"stream-browserify": "^3.0.0",
		"typography": "^0.16.19",
		"typography-theme-bootstrap": "^0.16.19"
	},
	"author": "Adam Flesher",
	"license": "ISC",

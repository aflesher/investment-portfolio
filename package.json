		"@types/react-dom": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^2.8.0",
		"@typescript-eslint/parser": "^2.8.0",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^8.0.3",
		"prettier": "2.0.5",
		"pretty-quick": "^3.1.1",
		"typescript": "^3.8.3"
	}
